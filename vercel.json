{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "http://senspulv.beget.tech/api/$1"
    },
    {
      "source": "/storage/(.*)",
      "destination": "http://senspulv.beget.tech/storage/$1"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
}
